{
  "countings": [
    {
      "name": "production_calls",
      "display_name": "Calls from the Production Department",
      "formula": {
        "column": "Dcontext",
        "regex": "production"
      },
      "exclude_other_matches": false
    },
    {
      "name": "after_business",
      "display_name": "Calls from the headquarter after business hours",
      "formula": {
        "operator": "and",
        "left": {
          "column": "Dcontext",
          "regex": "hq"
        },
        "right": {
          "column": "Start",
          "regex": "[0-9]{4}-[0-9]{2}-[0-9]{2} (18|19|20|21|22|23|24):[0-9]{2}:[0-9]{2}"
        }
      },
      "exclude_other_matches": true
    },
    {
      "name": "employees",
      "display_name": "Added calls from Magdalene Greenman and Farlie Brager",
      "formula": {
        "operator": "or",
        "left": {
          "column": "CallerId",
          "regex": ".*Magdalene Greenman.*"
        },
        "right": {
          "column": "CallerId",
          "regex": ".*Farlie Brager.*"
        }
      },
      "exclude_other_matches": false
    }
  ]
}